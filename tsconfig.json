{
    "version": "1.7",
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./www/app"
    },
    "filesGlob": [
        "./app/**/*.ts",
        "./app/**/*.tsx",
        "./typings/**.d.ts"
    ],
    "files": [
        "./app/AppConfig.ts",
        "./app/Events.ts",
        "./app/Extensions.ts",
        "./app/Logger.ts",
        "./app/Router.ts",
        "./app/bindingHandlers/Registration.ts",
        "./app/bindingHandlers/SlideVisible.ts",
        "./app/bootstrap.ts",
        "./app/components/Registration.ts",
        "./app/components/views/AboutView.ts",
        "./app/components/views/DashboardView.ts",
        "./app/components/views/NotFoundView.ts",
        "./app/components/views/ResumeView.ts",
        "./app/lib/jquery/JQueryScoped.ts",
        "./app/models/Route.ts",
        "./app/typings/Klochwork.d.ts",
        "./app/viewModels/ApplicationViewModel.ts",
        "./app/viewModels/ContentLoaderViewModel.ts",
        "./app/viewModels/NavigationViewModel.ts",
        "./app/viewModels/ResumeViewModel.ts",
        "./typings/tsd.d.ts"
    ]
}
